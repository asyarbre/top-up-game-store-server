<table id="example1" class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>No</th>
      <th>Nama Koin</th>
      <th>Jumlah Koin</th>
      <th>Harga Koin</th>
      <th>action</th>
    </tr>
  </thead>
  <tbody>
    <% for(let i=0; i<nominal.length; i++){ %>
      <tr>
        <td><%= i + 1 %></td>
        <td><%= nominal[i].coinName %></td>
        <td><%= nominal[i].CoinQuantity %></td>
        <td><%= nominal[i].price %></td>
        <td>
          <form action="/nominal/delete/<%= nominal[i]._id %>?_method=DELETE" method="post">
            <a href="/nominal/edit/<%= nominal[i]._id %>" class="btn btn-warning"><i class="fas fa-pencil-alt mr-1"></i>Ubah</a>
            <button class="btn btn-danger"><i class="fas fa-trash mr-1"></i>Hapus</button>
          </form>
        </td>
      </tr>
      <% } %>
  </tbody>
</table>
